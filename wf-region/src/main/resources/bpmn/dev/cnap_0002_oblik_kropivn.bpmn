        <activiti:formProperty id="sWorkbud_from" name="Початок роботи" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="sWorkbud_to" name="Закінчення роботи" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="sWorkdinbud_from" name="Початок перерви" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="sWorkdinbud_to" name="Закінчення перерви" type="long" required="true"></activiti:formProperty>
        <activiti:formProperty id="markersLine_2" name="Режим роботи об'єкта у вихідні дні" type="markers" default="{&quot;attributes&quot;:{&quot;Line_1&quot;:{&quot;aElement_ID&quot;:[&quot;sWorkholiday_from&quot;],&quot;sValue&quot;:&quot;Режим роботи об'єкта у вихідні дні&quot;}}}"></activiti:formProperty>
