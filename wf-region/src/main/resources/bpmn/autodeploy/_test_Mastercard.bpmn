      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="342.0" y="250.0"></omgdi:waypoint>
        <omgdi:waypoint x="311.0" y="250.0"></omgdi:waypoint>
