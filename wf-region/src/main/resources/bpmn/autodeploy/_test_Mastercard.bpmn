        <omgdi:waypoint x="362.0" y="230.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
        <omgdi:waypoint x="382.0" y="250.0"></omgdi:waypoint>
        <omgdi:waypoint x="414.0" y="250.0"></omgdi:waypoint>
