<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="_doc_comfy_dogovor" name="Комфи договор" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="processInstanceId" name="ід процесу; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sKey_Step_Document" name="sKey_Step_Document; ;bVisible=false" type="string"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <scriptTask id="scripttask1" name="id процесу" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var processInstanceId = execution.getProcessInstanceId();
execution.setVariable('processInstanceId', processInstanceId)</script>
    </scriptTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="scripttask1"></sequenceFlow>
    <userTask id="usertask1" name="создание автором">
      <extensionElements>
        <activiti:formProperty id="sTypeDoc" name="Тип документу" type="string" default="Договір" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sUnit" name="Оберіть підрозділ" type="enum" required="true">
          <activiti:value id="sOperational" name="Операційна дирекція"></activiti:value>
          <activiti:value id="sCommercial" name="Комерційна дирекція"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="sManagement" name="Оберіть управління" type="enum">
          <activiti:value id="sNetwork " name="Управління розвитку мережі"></activiti:value>
          <activiti:value id="sExploitation" name="Управління запуску та експлуатації торгових об'єктів"></activiti:value>
          <activiti:value id="sRetail" name="Роздрібна торгівля - магазини"></activiti:value>
        </activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${DocumentInit_iDoc}">
          <activiti:field name="sKey_GroupPostfix">
            <activiti:string><![CDATA[_default_Comfy]]></activiti:string>
          </activiti:field>
          <activiti:field name="sKey_GroupPostfix_New">
            <activiti:expression>${sLoginAuthor}</activiti:expression>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="scripttask1" targetRef="usertask1"></sequenceFlow>
    <scriptTask id="scripttask2" name="права руководителю автора, степ 2" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>//переключение степа
var snID_Process_Activiti = execution.getProcessInstanceId()
var sKey_Step= 'step_2'
var oResultSetDoc =
documentStepService.setDocumentStep(snID_Process_Activiti, sKey_Step)

//Клонування прав для керівника автора
var sKey_GroupPostfix = '_default_Comfy'
var sKey_GroupPostfix_New ='Comfy_Author's_Head'
var oResult = documentStepService.cloneDocumentStepSubject(snID_Process_Activiti,
sKey_GroupPostfix, sKey_GroupPostfix_New, sKey_Step)</script>
    </scriptTask>
    <sequenceFlow id="flow3" sourceRef="usertask1" targetRef="scripttask2"></sequenceFlow>
    <userTask id="usertask2" name="Рук автора">
      <extensionElements>
        <activiti:formProperty id="sTypeDoc" name="Тип документу" type="string" default="Договір" writable="false"></activiti:formProperty>
        <activiti:formProperty id="sDecision" name="Прийняте рішення" type="enum" required="true">
          <activiti:value id="accept" name="погодитися"></activiti:value>
          <activiti:value id="backToAuthor" name="повернути ініціатору"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow4" sourceRef="scripttask2" targetRef="usertask2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow5" sourceRef="usertask2" targetRef="endevent1"></sequenceFlow>
  </process>
  <bpmndi:BPMNDiagram id="BPMNDiagram__doc_comfy_dogovor">
    <bpmndi:BPMNPlane bpmnElement="_doc_comfy_dogovor" id="BPMNPlane__doc_comfy_dogovor">
      <bpmndi:BPMNShape bpmnElement="startevent1" id="BPMNShape_startevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="80.0" y="120.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="scripttask1" id="BPMNShape_scripttask1">
        <omgdc:Bounds height="55.0" width="105.0" x="160.0" y="110.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask1" id="BPMNShape_usertask1">
        <omgdc:Bounds height="55.0" width="105.0" x="310.0" y="110.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="scripttask2" id="BPMNShape_scripttask2">
        <omgdc:Bounds height="55.0" width="105.0" x="470.0" y="110.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask2" id="BPMNShape_usertask2">
        <omgdc:Bounds height="55.0" width="105.0" x="640.0" y="110.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent1" id="BPMNShape_endevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="470.0" y="220.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow1" id="BPMNEdge_flow1">
        <omgdi:waypoint x="115.0" y="137.0"></omgdi:waypoint>
        <omgdi:waypoint x="160.0" y="137.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="265.0" y="137.0"></omgdi:waypoint>
        <omgdi:waypoint x="310.0" y="137.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow3" id="BPMNEdge_flow3">
        <omgdi:waypoint x="415.0" y="137.0"></omgdi:waypoint>
        <omgdi:waypoint x="470.0" y="137.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="575.0" y="137.0"></omgdi:waypoint>
        <omgdi:waypoint x="640.0" y="137.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow5" id="BPMNEdge_flow5">
        <omgdi:waypoint x="692.0" y="165.0"></omgdi:waypoint>
        <omgdi:waypoint x="487.0" y="220.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>