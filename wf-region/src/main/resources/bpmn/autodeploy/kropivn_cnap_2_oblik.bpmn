  <process id="kropivn_cnap_2_oblik" name="kropivn_cnap_2_oblik" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="sID_Public_SubjectOrganJoin" name="Оберіть орган, у якому Вам буде зручно отримати відповідь" type="string" required="true"></activiti:formProperty>
