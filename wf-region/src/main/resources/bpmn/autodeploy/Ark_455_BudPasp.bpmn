        <activiti:formProperty id="bankIdAddressFactual" name="Домашня адреса" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdPassport" name="Паспортні дані" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="phone" name="Контактний телефон" type="string" default="+380" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="Ваш e-mail; На цей e-mail  Ви отримаєте результат обробки Вашого звернення" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="marker4" name="mark" type="markers" default="{&quot;motion&quot;:{&quot;ShowFieldsOnCondition_2&quot;:{&quot;aField_ID&quot;:[&quot;doc4&quot;],&quot;asID_Field&quot;:{&quot;sCond&quot;:&quot;spivvlasn&quot;},&quot;sCondition&quot;:&quot;[sCond]=='yes'&quot;}}}"></activiti:formProperty>
        <activiti:formProperty id="place" name="Адреса земельної ділянки" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="area" name="Площа земельної ділянки;Га" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="vlasnist" name="Документ, що посвідчує право власності або користування земельною ділянкою, або договор суперфіцію" type="enum" required="true">
          <activiti:value id="vlasnist1" name="договір купівлі-продажу приватизованої земельної ділянки"></activiti:value>
          <activiti:value id="vlasnist2" name="договір дарування приватизованої земельної ділянки"></activiti:value>
          <activiti:value id="vlasnist3" name="договір міни приватизованої земельної ділянки"></activiti:value>
          <activiti:value id="vlasnist4" name="свідоцтво про право власності на земельну ділянку"></activiti:value>
          <activiti:value id="vlasnist5" name="державний акт на право власності на земельну ділянку"></activiti:value>
          <activiti:value id="vlasnist6" name="державний акт на право постійного користування земельною ділянкою"></activiti:value>
          <activiti:value id="vlasnist7" name="договір оренди земельної ділянки"></activiti:value>
          <activiti:value id="vlasnist8" name="договір суперфіція"></activiti:value>
          <activiti:value id="vlasnist9" name="витяг з Державного реєстру речових прав на земельну ділянку"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="doc1" name="Засвідчена в установленому порядку копія документа про право власності або користування земельною ділянкою;згідно з роз`ясненням МінЮсту від 02.2013, на засвідченому документі проставляється прізвище і ініціали того, хто посвідчує, особистий підпис, дата і відмітка &quot;Згідно з оригіналом&quot;  ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="spivvlasn" name="Ви одноосібний власник ділянки?" type="enum" required="true">
          <activiti:value id="yes" name="ні, є інші співвласники"></activiti:value>
          <activiti:value id="no" name="так, ділянка належить лише мені"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="doc4" name="Засвідчена згода співвласників земельної ділянки (житлового будинку) на забудову;з ЕЦП нотаріуса ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="Message1" name="Увага" type="label" default="додайте необхідні файли!"></activiti:formProperty>
        <activiti:formProperty id="doc2" name="Ескізні наміри забудови;місце розташування будівель та споруд на земельній ділянці, відстані до меж сусідніх земельних ділянок та розташованих на них об`єктів, інженерних мереж і споруд, фасади та плани поверхів об`єктів із зазначенням габаритних розмірів, перелік систем інженерного забезпечення, у тому числі автономного, що планується до застосування, тощо. УВАГА! Ескізні наміри можуть бути виготовлені самостійно (згідно з перерахованими вище параметрами), або з залученням фахівців-архитекторів. ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankId_scan_passport" name="Копія паспорта замовника ; ;bNew=true" type="file" required="true"></activiti:formProperty>
        <activiti:formProperty id="nRegion" name="Регион; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:formProperty id="sArchShapka" name="Шапка заяви; ;bVisible=false" type="string"></activiti:formProperty>
