<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="sanepid" name="Довідка щодо підтримання належного санітарного стану" isExecutable="true">
    <startEvent id="startevent1" name="Start">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькові" type="string" required="true"></activiti:formProperty>
        <activiti:formProperty id="asSelectSexCheck" name="Виберіть будь-ласка свою стать" type="enum" required="true">
          <activiti:value id="man" name="мужчина"></activiti:value>
          <activiti:value id="girl" name="женщина"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="application_vopros1" name="Будь-ласка введіть свою найулюбленішу марку автомобіля" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="application_vopros2" name="Будь-ласка введіть свого найулюбленішого дизайнера одягу" type="textArea" required="true"></activiti:formProperty>
        <activiti:formProperty id="markers_Hide_01_01" name="показати питання 1, якщо це чоловік" type="markers" default="{&quot;motion&quot;: {ShowFieldsOnCondition_Hide_01_01&quot;: {aField_ID&quot;: [&quot;application_vopros1&quot;], asID_Field&quot;: {&quot;sClient&quot;: &quot;asSelectSexCheck&quot;}, &quot;sCondition&quot;: &quot;[sClient]== 'man'}}}" readable="false" required="true"></activiti:formProperty>
        <activiti:formProperty id="markers_Hide_01_02" name="показати питання 2, якщо це жінка" type="markers" default="{&quot;motion&quot;: {ShowFieldsOnCondition_Hide_01_02&quot;: {aField_ID&quot;: [&quot;application_vopros2&quot;], asID_Field&quot;: {&quot;sClient&quot;: &quot;asSelectSexCheck&quot;}, &quot;sCondition&quot;: &quot;[sClient]== 'girl'}}}" readable="false" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-Mail адреса" type="string" required="true"></activiti:formProperty>
      </extensionElements>
    </startEvent>
    <scriptTask id="scripttask1" name="Обробити данні" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var SelectSexCheck = execution.getVariable('asSelectSexCheck')
if (SelectSexCheck==man)
execution.setVariable (sHello, 'Доброго дня, мужчина!')
else
execution.setVariable (sHello, 'Доброго дня, дівчино!')

</script>
    </scriptTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="scripttask1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="servicetask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${SelectSexCheck==man}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask1" name="Відправити лист чоловікові" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[tests@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[Ваші відповіді прийняті.]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[${vFile}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[${sHello}]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="servicetask2" name="Відправити лист жінці" activiti:delegateExpression="#{MailTaskWithAttachments}">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression><![CDATA[${email}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[tests@gmail.com]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
          <activiti:expression><![CDATA[Ваші відповіді прийняті.]]></activiti:expression>
        </activiti:field>
        <activiti:field name="saAttachmentsForSend">
          <activiti:expression><![CDATA[${vFile}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="text">
          <activiti:expression><![CDATA[${sHello}]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="servicetask1" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="servicetask2" targetRef="exclusivegateway2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow8" sourceRef="exclusivegateway2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway1" targetRef="servicetask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${SelectSexCheck==girl}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask3" name="Продивитись відповіді" activiti:assignee="kermit">
      <extensionElements>
        <activiti:formProperty id="bankIdlastName" name="Прізвище" type="string" readable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdfirstName" name="Ім'я" type="string" readable="false"></activiti:formProperty>
        <activiti:formProperty id="bankIdmiddleName" name="По-батькові" type="string" readable="false"></activiti:formProperty>
        <activiti:formProperty id="asSelectSexCheck" name="Результат вибраної статі" type="enum" required="true">
          <activiti:value id="man" name="Чоловік"></activiti:value>
          <activiti:value id="girl" name="Жінка"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="vFile" name="Документ з рекомендаціями; ;bNew=true" type="file" readable="false" required="true"></activiti:formProperty>
        <activiti:formProperty id="email" name="E-mail адреса" type="string" readable="false"></activiti:formProperty>
        <activiti:formProperty id="sHello" name="Ексклюзивне вітання; ;bVisible=false" type="string"></activiti:formProperty>
        <activiti:taskListener event="create" delegateExpression="${fileTaskUploadListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow10" sourceRef="usertask3" targetRef="scripttask1"></sequenceFlow>
    <sequenceFlow id="flow11" sourceRef="startevent1" targetRef="usertask3"></sequenceFlow>
  </process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_sanepid">
    <bpmndi:BPMNPlane bpmnElement="sanepid" id="BPMNPlane_sanepid">
      <bpmndi:BPMNShape bpmnElement="startevent1" id="BPMNShape_startevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="1.0" y="92.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="scripttask1" id="BPMNShape_scripttask1">
        <omgdc:Bounds height="55.0" width="105.0" x="209.0" y="80.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="exclusivegateway1" id="BPMNShape_exclusivegateway1">
        <omgdc:Bounds height="40.0" width="40.0" x="359.0" y="88.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="servicetask1" id="BPMNShape_servicetask1">
        <omgdc:Bounds height="55.0" width="105.0" x="446.0" y="38.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="servicetask2" id="BPMNShape_servicetask2">
        <omgdc:Bounds height="55.0" width="105.0" x="446.0" y="124.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="exclusivegateway2" id="BPMNShape_exclusivegateway2">
        <omgdc:Bounds height="40.0" width="40.0" x="594.0" y="89.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="endevent1" id="BPMNShape_endevent1">
        <omgdc:Bounds height="35.0" width="35.0" x="679.0" y="92.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape bpmnElement="usertask3" id="BPMNShape_usertask3">
        <omgdc:Bounds height="55.0" width="105.0" x="69.0" y="82.0"></omgdc:Bounds>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge bpmnElement="flow2" id="BPMNEdge_flow2">
        <omgdi:waypoint x="314.0" y="107.0"></omgdi:waypoint>
        <omgdi:waypoint x="359.0" y="108.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="379.0" y="88.0"></omgdi:waypoint>
        <omgdi:waypoint x="379.0" y="65.0"></omgdi:waypoint>
        <omgdi:waypoint x="446.0" y="65.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow6" id="BPMNEdge_flow6">
        <omgdi:waypoint x="551.0" y="65.0"></omgdi:waypoint>
        <omgdi:waypoint x="614.0" y="65.0"></omgdi:waypoint>
        <omgdi:waypoint x="614.0" y="89.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow7" id="BPMNEdge_flow7">
        <omgdi:waypoint x="551.0" y="151.0"></omgdi:waypoint>
        <omgdi:waypoint x="614.0" y="151.0"></omgdi:waypoint>
        <omgdi:waypoint x="614.0" y="129.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow8" id="BPMNEdge_flow8">
        <omgdi:waypoint x="634.0" y="109.0"></omgdi:waypoint>
        <omgdi:waypoint x="679.0" y="109.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow9" id="BPMNEdge_flow9">
        <omgdi:waypoint x="379.0" y="128.0"></omgdi:waypoint>
        <omgdi:waypoint x="379.0" y="151.0"></omgdi:waypoint>
        <omgdi:waypoint x="446.0" y="151.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow10" id="BPMNEdge_flow10">
        <omgdi:waypoint x="174.0" y="109.0"></omgdi:waypoint>
        <omgdi:waypoint x="209.0" y="107.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow11" id="BPMNEdge_flow11">
        <omgdi:waypoint x="36.0" y="109.0"></omgdi:waypoint>
        <omgdi:waypoint x="69.0" y="109.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>